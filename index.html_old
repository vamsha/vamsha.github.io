<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>AI Agents and NLP Journey</title>
    <link rel="stylesheet" href="styles.css">
    <!-- Load Marked.js from CDN -->
    <script src="https://cdn.jsdelivr.net/npm/marked/marked.min.js"></script>
</head>
<body>
    <header>
        <h1>Welcome to the NLP and AI Agents Journey</h1>
        <p>Explore the world of Natural Language Processing and AI Agents, from the basics to Large Language Models.</p>
    </header>

    <main id="readme-content">
        <!-- The Markdown content will be injected here -->
    </main>

    <script>
        // Fetch the README.md file from the raw GitHub URL
        fetch('https://raw.githubusercontent.com/vamsha/vamsha.github.io/main/README.md')
            .then(response => response.text()) // Get the raw text of the markdown file
            .then(markdown => {
                // Convert the raw Markdown to HTML using Marked.js
                const htmlContent = marked(markdown);

                // Inject the converted HTML into the <main> section
                document.getElementById('readme-content').innerHTML = htmlContent;
            })
            .catch(error => console.error('Error loading README.md:', error));
    </script>
</body>
</html>
